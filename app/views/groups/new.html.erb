<div class="form-group col-md-4 col-md-offset-4">
  <h2>新增討論</h2>
  <hr>
  <%= form_for @group do |f| %>
  標題
  <%= f.text_field :title, class:'form-control' %>
  <br>
  敘述
  <br>
  <%= f.text_area :description, class:'form-control' %>
  <br>
  <%= f.submit "Submit", :disable_with => 'Submiting...', class:'btn btn-primary' %>
  <% end %>
</div>
